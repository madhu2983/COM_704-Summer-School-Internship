//9.Second Largest 1
using namespace std;
int main() {
    int a, b, c;
    char comma;
    cin >> a >> comma >> b >> comma >> c;

    int second_largest;
    if ((a >= b && a <= c) || (a >= c && a <= b)) {
        second_largest = a;
    } else if ((b >= a && b <= c) || (b >= c && b <= a)) {
        second_largest = b;
    } else {
        second_largest = c;
    }
    cout << second_largest << endl;

    return 0;
}
